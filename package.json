{
  "name": "mrat-dashboard",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview --host",
    "lint": "eslint .",
    "server": "ts-node server/index.ts",
    "server:dev": "nodemon --exec ts-node server/index.ts",
    "dev:full": "concurrently \"npm run dev\" \"npm run server:dev\"",
    "seed": "ts-node server/seed.ts",
    "build:server": "tsc -p server/tsconfig.json",
    "start:server": "node dist/server/index.js",
    "start": "npm run build:server && npm run start:server"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "cors": "^2.8.5",
    "date-fns": "^3.3.1",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "lucide-react": "^0.344.0",
    "mongoose": "^8.15.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.2",
    "recharts": "^2.12.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.18",
    "@types/express": "^4.17.22",
    "@types/node": "^22.15.29",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "nodemon": "^3.1.10",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^5.1.3"
  }
}
